<h1>Users</h1>

<table id="review-table">
<tr>
  <th>Username</th>
  <th>Last Login</th>
  <th>User Level</th>
  <th>Status</th>
</tr>
<% @users.each do |u| %>
  <tr>
		<td><%= link_to u.username, edit_user_path(u) %></td>
		<td>
			<% if u.last_login.nil? %>
      User has not logged in yet.
    <% else %>
      <%= u.last_login.strftime("%B %-d, %Y at %l:%M %p") %>
    <% end %>
		</td>
		<td><%= u.user_level.level %></td>
    <td><%=u.user_status.status%></td>
  </tr>
<% end %>
</table>

<%= link_to 'Register New User', register_path, class: 'link_button' %>