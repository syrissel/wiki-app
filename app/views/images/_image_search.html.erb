<div id="image_search">
  <%= simple_form_for new_page_path, method: :get, remote: true do |f| %>
		<%= f.text_field :imageq %>
		<%= f.submit 'Search'  %>
	<% end %>
	<div class="media_options_upload_wrapper"><%= link_to 'Upload', new_image_path, remote: true, class: 'media_options_upload' %></div>
	<div id="images">
		<%= render @images %>
	</div>
</div>